{
  "required": [
    "birth_date",
    "birth_time",
    "birth_location",
    "iana_time_zone"
  ],
  "properties": {
    "birth_date": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
    },
    "birth_time": {
      "type": "string",
      "pattern": "^\\d{2}:\\d{2}(:\\d{2})?$"
    },
    "birth_location": {
      "type": "object",
      "required": [
        "lat",
        "lon"
      ],
      "properties": {
        "lat": {
          "type": "number"
        },
        "lon": {
          "type": "number"
        }
      }
    },
    "iana_time_zone": {
      "type": "string"
    },
    "house_system": {
      "type": "string",
      "default": "P"
    },
    "strict_mode": {
      "type": "boolean",
      "default": true
    },
    "ut1_minus_utc_seconds": {
      "type": "number",
      "default": 0.0
    }
  }
}